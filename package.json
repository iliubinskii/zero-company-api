{
  "name": "zero-company-api",
  "version": "0.1.0",
  "private": true,
  "description": "Zero Company API",
  "keywords": [
    "crowdworking"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iliubinskii/zero-company-api"
  },
  "license": "Proprietary",
  "author": "Ilia Liubinskii <ilia.liubinskii@gmail.com>",
  "contributors": [
    {
      "name": "Alex Feldshteyn",
      "email": "alexey.israel22@gmail.com"
    },
    {
      "name": "David Liubinskii",
      "email": "workdltest@gmail.com"
    },
    {
      "name": "Ilia Liubinskii",
      "email": "ilia.liubinskii@gmail.com"
    }
  ],
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build-dummy-data": "tsx utils/dummy-data.ts && npm run format",
    "contributors": "python -m gitfame --bytype --excl \"\\.(ico|json|svg)$\" ./src >./CONTRIBUTORS.md",
    "deploy-preview": "vercel",
    "deploy-prod": "vercel --prod",
    "dev": "nodemon",
    "docker:build": "docker build -t zero-company-api .",
    "docker:start": "docker run --env-file .env -p 3000 zero-company-api",
    "format": "prettier --write \"**/*.{css,html,json,less,postcss,scss}\"",
    "install-deps": "npm install --save-prod --strict-peer-deps",
    "install-dev-deps": "npm install --save-dev --strict-peer-deps",
    "lint": "eslint --fix --max-warnings=0 .",
    "lint-profile": "cross-env TIMING=1 eslint --max-warnings=0 .",
    "lint-style": "stylelint --fix --max-warnings=0 src/**/*.{css,less,postcss,scss}",
    "ncu-check": "ncu --dep=dev,peer,prod",
    "ncu-upgrade": "ncu --dep=dev,peer,prod --upgrade && npm run install-deps",
    "parse-openapi": "npm run parse-openapi-to-json-1 && npm run parse-openapi-to-json-2 && npm run parse-openapi-to-routes",
    "parse-openapi-to-json-1": "swagger-cli bundle public/schema.yaml -o public/schema.json && prettier --write public/schema.json",
    "parse-openapi-to-json-2": "swagger-cli bundle public/schema.yaml -o src/schema/schema.json && prettier --write src/schema/schema.json",
    "parse-openapi-to-routes": "openapi-typescript public/schema.yaml -o src/schema/routes.ts && eslint --fix src/schema/routes.ts",
    "prepare": "husky",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "type-check": "tsc --incremental false --noEmit"
  },
  "lint-staged": {
    "*.{css,html,json,less,postcss,scss}": "prettier --write",
    "*.{css,less,postcss,scss}": "stylelint --max-warnings=0",
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/multer": "^1.4.11",
    "@types/passport": "^1.0.16",
    "@types/passport-auth0": "^1.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "cloudinary": "^2.2.0",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "glob-to-regexp": "^0.4.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.4.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.14",
    "ts-toolbelt": "^9.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.3",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/stylelint": "^14.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
