{
  "name": "zero-company-api",
  "version": "0.1.0",
  "private": true,
  "description": "Zero Company API",
  "keywords": [
    "crowdworking"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iliubinskii/zero-company-api"
  },
  "author": "Ilia Liubinskii",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "dev": "nodemon",
    "format": "prettier --write \"**/*.{css,html,json,less,postcss,scss}\"",
    "install-deps": "npm install --strict-peer-deps",
    "install-dev-deps": "npm install --save-dev --strict-peer-deps",
    "lint": "eslint --fix --max-warnings=0 .",
    "lint-style": "stylelint --fix --max-warnings=0 src/**/*.{css,less,postcss,scss}",
    "prepare": "husky",
    "start": "node dist/index.js",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i18next": "^23.11.2",
    "tsx": "^4.7.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/stylelint": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-sort-annotation": "^1.0.6",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-imports-requires": "^1.0.2",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-sort-json": "^4.0.0",
    "react": "^18.2.0",
    "stylelint": "^16.3.1",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
